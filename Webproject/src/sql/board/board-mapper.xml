<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
	<comment>일반게시판 관련 쿼리문</comment>
	
	<entry key="selectListCount">
		SELECT COUNT(*)
		FROM BOARD
		WHERE STATUS ='Y'
		AND BOARD_TYPE =1
	</entry>
	
	
</properties>